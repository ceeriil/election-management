<a href="/" class="btn btn-info" style="margin-top: 5px">Back</a>
<h3>Polling Agent Login</h3>
<div class="agentLog">
  <form method="POST" class="agentForm">
    <div class="mb-3">
      <label for="exampleInputPassword1" class="form-label">Password : </label>
      <input type="password" name="password" class="form-control" id="pass" />
      <br />
      <input type="checkbox" style="width: 30px" onclick="myFunction()" />Show
      Password
      <br />
    </div>
    <a><button type="submit" class="btn btn-primary">Submit</button></a>
  </form>
</div>
<script>
  function myFunction() {
    var x = document.getElementById("pass");
    if (x.type === "password") {
      x.type = "text";
    } else {
      x.type = "password";
    }
  }
</script>
